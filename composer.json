{
  "name": "cspray/labrador-http-cors",
  "description": "An Amp http-server middleware library to facilitate CORS",
  "scripts": {
    "code-lint": "vendor/bin/labrador-cs src/ test/",
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": ">= 8.1",
    "amphp/http-server": "^3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "amphp/phpunit-util": "^3",
    "cspray/labrador-coding-standard": "0.2.0"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Labrador\\Http\\Cors\\": ["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Labrador\\Http\\Cors\\": ["test/"]
    }
  }
}
